{
    "name": "qdrant-js",
    "version": "0.0.x",
    "private": true,
    "engines": {
        "node": ">=18.0.0",
        "pnpm": ">=8"
    },
    "packageManager": "pnpm@8.1.1",
    "license": "Apache-2.0",
    "type": "module",
    "main": "./dist/cjs/index.js",
    "module": "./dist/esm/index.js",
    "browser": "./dist/browser/index.js",
    "types": "./dist/types/index.d.ts",
    "exports": {
        "import": "./dist/esm/index.js",
        "require": "./dist/cjs/index.js",
        "browser": "./dist/browser/index.js",
        "types": "./dist/types/index.d.ts"
    },
    "files": [
        "dist"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/qdrant/qrant-js"
    },
    "bugs": {
        "url": "https://github.com/qdrant/qrant-js/issues"
    },
    "homepage": "https://github.com/qdrant/qrant-js#readme",
    "scripts": {
        "test": "echo 'test'",
        "prepare": "pnpm husky install",
        "prerelease": "pnpm pre-commit && pnpm tsc:build && pnpm build",
        "pre-check": "pnpm tsc:check && pnpm lint && pnpm test",
        "pre-commit": "pnpm tsc:check && pnpm lint-staged && pnpm test",
        "tsc:check": "tsc --noEmit",
        "tsc:build": "pnpm clean && tsc -p tsconfig.json && tsc -p tsconfig.cjs.json",
        "build": "rollup -c rollup.config.js",
        "lint": "eslint src",
        "clean": "rm -rf ./dist",
        "codegen:openapi-typescript": "openapi-typescript $npm_package_config_openapi_schema_remote --output src/openapi/generated_schema.ts",
        "codegen:buf": "buf generate proto",
        "openapi-mock-server": "docker run -p 6333:6333 -e \"OPENAPI_MOCK_DEBUG=1\" -e \"OPENAPI_MOCK_LOG_LEVEL=debug\" -e \"OPENAPI_MOCK_PORT=6333\" -e \"OPENAPI_MOCK_SPECIFICATION_URL=$npm_package_config_openapi_schema_remote\" --rm muonsoft/openapi-mock"
    },
    "config": {
        "openapi_schema_remote": "https://raw.githubusercontent.com/qdrant/qdrant/master/docs/redoc/master/openapi.json"
    },
    "publishConfig": {
        "access": "restricted"
    },
    "lint-staged": {
        "*.ts": [
            "prettier --list-different",
            "eslint"
        ],
        "*.{md,yaml}": "prettier --list-different"
    },
    "devDependencies": {
        "@bufbuild/buf": "1.15.0-1",
        "@bufbuild/protoc-gen-connect-es": "0.8.4",
        "@bufbuild/protoc-gen-es": "1.2.0",
        "@commitlint/cli": "17.5.1",
        "@commitlint/config-conventional": "17.4.4",
        "@rollup/plugin-node-resolve": "15.0.2",
        "@rollup/plugin-terser": "0.4.0",
        "@total-typescript/ts-reset": "0.4.2",
        "@types/node": "18.15.11",
        "@typescript-eslint/eslint-plugin": "5.57.0",
        "@typescript-eslint/parser": "5.57.0",
        "husky": "8.0.3",
        "lint-staged": "13.2.1",
        "openapi-typescript": "6.2.0",
        "prettier": "2.8.7",
        "rollup": "3.20.2",
        "ts-node": "10.9.1",
        "typescript": "5.0.2",
        "vitest": "0.29.8"
    },
    "dependencies": {
        "@sevinf/maybe": "0.5.0",
        "openapi-typescript-fetch": "Rendez/openapi-typescript-fetch"
    },
    "peerDependencies": {
        "typescript": ">=4.1"
    },
    "optionalDependencies": {
        "@bufbuild/connect": "0.8.4",
        "@bufbuild/connect-node": "0.8.4",
        "@bufbuild/protobuf": "1.2.0"
    }
}
