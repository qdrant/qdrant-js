{
    "private": true,
    "name": "@qdrant/monorepo",
    "version": "0.0.0",
    "engines": {
        "node": ">=18.0.0",
        "pnpm": ">=8"
    },
    "packageManager": "pnpm@8.3.0",
    "scripts": {
        "prepare": "node ./prepare.cjs && pnpm -r build",
        "release": "pnpm -r build && pnpm changeset publish",
